#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int i;
    char ch;
    char s[100],sen[100];
    
    scanf("%c",&ch);
    printf("%c\n",ch);

    scanf("%s",s);
    printf("%s\n",s);

    scanf("\n");
    scanf("%[^\n]%*c",sen);
    printf("%s",sen);
    
    return 0;
}
